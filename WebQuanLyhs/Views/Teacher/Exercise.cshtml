@model IEnumerable<BusinessObject.Data.Exercise>

@{
    ViewData["Title"] = "Exercise";
}
@{
    Layout = "_LayoutStudentandTeacher";
}



<div class="card-body">
    <div class="background-content">
        <div class="clearfix mb-3">
            <a class="btn btn-success float-end" style="color: white;" asp-action="ExerciseAdd">Create New</a>
        </div>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Exercise_name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Creat_time)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.File_name)
            </th>
         
            <th>
                @Html.DisplayNameFor(model => model.Course)
            </th>
           
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Exercise_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Creat_time)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.File_name)
            </td>
            
                <td>
                    @Html.DisplayFor(modelItem => item.Course.Coures_name)
                </td>
      
        </tr>
}
    </tbody>
</table>
</div>
</div>
